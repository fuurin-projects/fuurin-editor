{
  "name": "editor",
  "version": "0.0.2",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "chcp 65001 && electron ../../build/app/",
    "copy_html": "cpx -C  ./src/main/html/** ../../build/app/html/",
    "copy_resources": "cpx -C  ./src/main/resources/**/{*.html,*.js} ../../build/app/resources/",
    "clean_svg": "svgo --config=./svgo.yaml -f ./src/main/resources/images",
    "compile": "npx webpack",
    "watch": "npx webpack --watch",
    "build": "node build-win.js",
    "assembly": "npm-run-all copy_html copy_resources compile build",
    "storybook": "start-storybook -p 6006 --no-dll",
    "build-storybook": "build-storybook --no-dll"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.4.0",
    "@types/express": "^4.17.6",
    "@types/react": "^16.9.21",
    "@types/react-dom": "^16.9.5",
    "@types/react-redux": "^7.1.9",
    "electron": "^8.5.2",
    "electron-devtools-installer": "^3.2.0",
    "express": "^4.17.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.0.28",
    "@storybook/addon-essentials": "^6.0.28",
    "@storybook/addon-links": "^6.0.28",
    "@storybook/react": "^6.0.28",
    "@svgr/webpack": "^5.5.0",
    "@types/electron-devtools-installer": "^2.2.0",
    "cpx": "^1.5.0",
    "css-loader": "^3.4.2",
    "electron-builder": "^22.3.2",
    "file-loader": "^6.1.0",
    "npm-run-all": "^4.1.5",
    "storybook": "^6.1.10",
    "style-loader": "^1.1.3",
    "svgo": "^1.3.2",
    "ts-loader": "^6.2.1",
    "typescript": "^3.9.6",
    "url-loader": "^4.1.1",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11"
  }
}
