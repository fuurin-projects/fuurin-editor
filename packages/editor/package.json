    "watch": "npx webpack --watch",
    "build": "node build-win.js"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.4.0",
    "@types/express": "^4.17.6",
    "@types/react": "^16.9.21",
    "@types/react-dom": "^16.9.5",
    "@types/react-redux": "^7.1.9",
    "electron": "^8.5.1",
    "express": "^4.17.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5"
  },
  "devDependencies": {
    "cpx": "^1.5.0",
    "css-loader": "^3.4.2",
    "electron-builder": "^22.3.2",
    "style-loader": "^1.1.3",
    "svgo": "^1.3.2",
    "ts-loader": "^6.2.1",
    "typescript": "^3.9.6",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11"
  }
